{
  "name": "robotjs",
  "version": "0.4.4",
  "description": "Node.js Desktop Automation.",
  "main": "index.js",
  "scripts": {
    "test": "node test/all.js",
    "preinstall": "npm install node-pre-gyp",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octalmage/robotjs.git"
  },
  "keywords": [
    "Automation",
    "GUI",
    "mouse",
    "keyboard",
    "screenshot",
    "image",
    "pixel",
    "desktop",
    "robotjs",
    "screen",
    "recognition",
    "autohotkey",
    "machine",
    "learning",
    "color"
  ],
  "author": "Jason Stallings",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/octalmage/robotjs/issues"
  },
  "homepage": "https://github.com/octalmage/robotjs",
  "dependencies": {
    "nan": "^2.2.1",
    "node-pre-gyp": "0.6.x"
  },
  "devDependencies": {
    "tape": "^3.5.0",
    "aws-sdk": "2.x"
  },
  "binary": {
       "module_name" : "robotjs",
       "module_path" : "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
       "remote_path" : "./{module_name}/v{version}/{configuration}/",
       "host"        : "https://robotjs-builds.s3-us-east-1.amazonaws.com"
   }
}
